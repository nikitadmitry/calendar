{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AgendaViewer": {
    "ApiUrl": "http://agenda-viewer/",
    "CircuitBreaker": {
      "IsActive": false
    }
  },
  "AgendaScheduler": {
    "ApiUrl": "http://agenda-scheduler/",
    "CircuitBreaker": {
      "IsActive": false
    }
  },
  "Kafka": {
    "Servers": "kafka:9092",
    "ConsumerGroupId": "calendar-webapp",
    "Topics": {
      "EventAddedMessage": "scheduler_event_added",
      "EventRemovedMessage": "scheduler_event_removed"
    }
  }
}