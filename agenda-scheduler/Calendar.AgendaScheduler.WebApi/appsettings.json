{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Mongo": {
    "DatabaseName": "AgendaSchedule",
    "ConnectTimeoutSeconds": 5,
    "Url": "mongodb://mongo:27017"
  },
  "Kafka": {
    "Servers": "kafka:9092",
    "Topics": {
      "EventAddedMessage": "scheduler_event_added",
      "EventRemovedMessage": "scheduler_event_removed"
    }
  }
}